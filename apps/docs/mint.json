{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Feedbase Docs",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/logo/favicon.svg",
  "colors": {
    "primary": "#8F9EFF",
    "light": "#e1e1e1",
    "dark": "#06060A",
    "background": {
      "light": "#fff",
      "dark": "#06060A"
    },
    "anchors": {
      "from": "#030303",
      "to": "#030303"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "topbarLinks": [
    {
      "name": "Sign In",
      "url": "https://dash.feedbase.app/login"
    }
  ],
  "topbarCtaButton": {
    "name": "Get Started",
    "url": "https://dash.feedbase.app/signup"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    },
    { 
      "name": "SDK Reference",
      "url": "sdk-reference"
    }
  ],
  "anchors": [
    {
      "name": "Github",
      "icon": "github",
      "url": "https://feedbase.app/github"
    },
    {
      "name": "Public Hub",
      "icon": "people-group",
      "url": "https://hub.feedbase.app"
    },
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://feedbase.app/discord"
    }
  ],
  "navigation": [
    {
      "group": "Introduction",
      "pages": ["introduction"]
    },
    {
      "group": "Development Guide",
      "pages": ["development/installation", "development/configuration"]
    },
    {
      "group": "Self Hosting",
      "pages": ["self-hosting/vercel", "self-hosting/docker"]
    }
  ],
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "footerSocials": {
    "twitter": "https://twitter.com/0xChroxify",
    "github": "https://github.com/chroxify/feedbase"
  }
}
